{
	"root": true,
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"airbnb",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"sourceType": "script",
    "ecmaFeatures": {
        "jsx": true
    }
	},
	// "globals": {},
  "plugins": [
    "react"
  ],
	"rules": {
		"linebreak-style": "off",
		"no-var": "off",
    "indent": [ "error", "tab"],
    "no-tabs": "off",
		"react/jsx-filename-extension": "off",
		"no-console": "off"
	}
}